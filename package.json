    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "styled-components": "^3.4.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
